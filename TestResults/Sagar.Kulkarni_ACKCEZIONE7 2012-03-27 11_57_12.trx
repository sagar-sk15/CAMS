       Description NVARCHAR(255) null,
       primary key (LogId)
    )

    create table Addresses (
        AddressId BIGINT IDENTITY NOT NULL,
       AddressLine1 NVARCHAR(255) null,
       AddressLine2 NVARCHAR(255) null,
